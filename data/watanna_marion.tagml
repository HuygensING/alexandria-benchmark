[excerpt|+A,+A2,+B type='novel' title='Marion, the story of an artist\'s model' author->wat01 year=1916 page=[17,18,19] persons=[{:id=mar01 name='Marion Ascough'},{:id=ada01 name='Ada Ascough'},{:id=ell01 name='Ellen Ascough'},{:id=stv01 name='Monsieur de St. Vidal'},{:id=wat01 name='Onoto Watanna'},{:id=wall01 name='Wallace'}] source_id='urn:oclc:record:1048793515'>
